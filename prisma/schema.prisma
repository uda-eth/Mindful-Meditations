 generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model JournalEntry {
  id         Int      @id @default(autoincrement())
  transcript String   @db.Text
  createdAt  DateTime @default(now())

  @@index([createdAt(sort: Desc)])
}
